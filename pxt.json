{
    "name": "pxt-lightplay",
    "description": "",
    "dependencies": {
        "core": "*",
        "bluetooth": "*",
        "pxt-scratch-microbit-more": "file:../pxt-scratch-microbit-more"
    },
    "files": [
        "main.blocks",
        "main.ts",
        "README.md"
    ],
    "public": true,
    "supportedTargets": [
        "microbit"
    ],
    "preferredEditor": "tsprj",
    "yotta": {
        "config": {
            "microbit-dal": {
                "bluetooth": {
                    "enabled": 1,
                    "pairing_mode": 1,
                    "private_addressing": 0,
                    "open": 1,
                    "whitelist": 1,
                    "advertising_timeout": 0,
                    "tx_power": 0,
                    "dfu_service": 0,
                    "event_service": 0,
                    "device_info_service": 0,
                    "security_level": "SECURITY_MODE_ENCRYPTION_NO_MITM"
                },
                "gatt_table_size": "0x340"
            }
        }
    }
}
